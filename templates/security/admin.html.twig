{% extends 'base.html.twig' %}

{% block title %}Admin{% endblock %}

{% block body %}
    <div class="container">
    {% for user in users %}
        <div class="card bg-light text-black mt-3">
            <div class="card-body">
                <div><a href="{{ path('admin_see_profile',{'user': user.id}) }}">Author: {{ user.name }}</a></div>
            </div>
        </div>
    {% endfor %}
    </div>
{% endblock %}